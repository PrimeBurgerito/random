{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "ignorePatterns": ["node_modules"],
  "rules": {
    "quotes": ["error", "single"],
    "object-curly-spacing": ["error", "always"]
  }
}
